.ui-toast {
  display: none;
  margin-left: 0;
  margin-right: 0;
  margin-bottom: 0;
  font-size: 34px;
  border-top: 2px solid B061L3;
  position: fixed;
  bottom: 0;
  height: 160px;
  width: 100%;
  z-index: 1100;
}
.ui-toast .ui-toast-wrapper {
  height: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  -webkit-flex-direction: column;
}
.ui-toast .ui-toast-content {
  flex: 1;
  -webkit-flex: 1;
  min-height: initial;
  background-color: B061L1;
  font-size: 28px;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  -webkit-flex-direction: column;
  align-items: center;
  -webkit-align-items: center;
  text-align: center;
  padding: 6px;
  overflow: hidden;
}
.ui-toast.in {
  display: block;
}
.ui-toast.ui-toast-active {
  display: block;
}
/****************************************
