****************************************/
.ui-toast-overlay {
  position: fixed;
  display: none;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.6);
}
.ui-toast-overlay.in {
  display: block;
}
