  height: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  -webkit-flex-direction: column;
}
.ui-popup:not(.ui-ctxpopup) {
  bottom: 0;
}
.ui-popup .ui-popup-header {
  min-height: 46px;
  height: 46px;
  line-height: 44px;
  color: T050;
  font-size: 28px;
  white-space: nowrap;
  overflow: hidden;
  text-align: center;
  margin: 0;
  padding: 0 6px;
  border-bottom: 2px solid B061L2;
}
.ui-popup .ui-popup-content {
  width: 100%;
  min-height: 224px;
  font-size: 30px;
  color: T053;
  overflow: auto;
  flex: 1;
  -webkit-flex: 1;
  padding: 12px 6px;
  text-align: center;
  -webkit-overflow-scrolling: touch;
  -moz-overflow-scrolling: touch;
  -o-overflow-scrolling: touch;
  -ms-overflow-scrolling: touch;
  overflow-scrolling: touch;
}
.ui-popup .ui-popup-footer {
  width: 100%;
}
.ui-popup .ui-popup-footer .ui-btn {
  font-size: 34px;
}
.ui-popup .ui-popup-footer .ui-btn.ui-btn-icon-only {
  position: relative;
  text-indent: -9999px;
}
.ui-popup .ui-popup-footer .ui-btn.ui-btn-icon-only::before {
  content: "";
  position: absolute;
  -webkit-mask-size: 50px 50px;
  -webkit-mask-repeat: no-repeat;
  width: 50px;
  height: 50px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
}
.ui-popup.ui-popup-active {
  display: block;
}
.ui-popup.in {
  display: block;
}
.ui-popup.ui-build {
  display: block;
  visibility: hidden;
}
