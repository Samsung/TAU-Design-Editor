body {
  height: 100%;
  font-size: 30px;
  font-family: Tizen, Samsung Sans, Helvetica;
  background: B011;
  color: T128;
}
.ui-page {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  display: none;
  background: B011;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
.ui-page.ui-page-active {
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  -webkit-flex-direction: column;
}
