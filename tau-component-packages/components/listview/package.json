{
  "label": "Listview",
  "selector": ".ui-listview",
  "attachable": true,
  "draggable": true,
  "type": "container-component",
  "displayOrderWeight": 500,
  "resources": {
    "css": [
      "../../tau/theme/tau.min.css",
      "../../tau/theme/tau.circle.min.css",
      "./styles/style.css"
    ],
    "js": "../../tau/tau.min.js",
    "icon": {
      "mobile": "./mobile-listview-w.jpg",
      "tv": "./mobile-listview-w.jpg",
      "wearable": "./wearable-listview-c.png"
    }
  },
  "template": "<ul class=\"ui-listview\" data-colored-background=\"false\"><li><span>List Item</span></li></ul>",
  "constraint": [
    "listitem"
  ],
  "class": [
    {
      "text": "ui-colored-list",
      "description": "Listview with gradient background",
      "version": "mobile"
    },
    "ui-snap-listview"
  ],
  "data": [
    {
      "option": "autodividers",
      "values": [
        {
          "snippet": "${1:0}",
          "leftLabelHTML": "boolean",
          "description": "set min value of progress"
        }
      ],
      "version": "mobile@2.2"
    },
    {
      "option": "inset",
      "values": [
        {
          "snippet": "${1:0}",
          "leftLabelHTML": "boolean"
        }
      ],
      "version": "mobile@2.2"
    },
    {
      "option": "fastscroll",
      "values": [
        {
          "snippet": "${1:0}",
          "leftLabelHTML": "boolean"
        }
      ],
      "version": "mobile@2.2"
    }
  ]
}
